<div>
  <form>
    <label>Materials</label>
    <ul>
      <% @materials.each do |material| %>
        <li>
          <%= material.name %>
          <input type='number' name='total_weight' value='0'>
        </li>

      <% end %>
    </ul>
    <input type='submit' value='Upload'>
  </form>
</div>
